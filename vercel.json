{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "ALLOW-FROM https://your-allowed-origin.com"
        },
        {
          "key": "Content-Security-Policy",
          "value": "frame-ancestors https://your-allowed-origin.com"
        }
      ]
    }
  ]
}
